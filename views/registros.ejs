<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta content="IE=edge" http-equiv="X-UA-Compatible" />
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <link rel="stylesheet" href="/css/styles.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
    <title>Resultados de la Búsqueda</title>
  </head>
  <body>
    <div class="content">
      <div class="container" id="card-results">
      
        <div class="htable">
          <h1 class="gradient__text">Resultados de la Búsqueda</h1>
        </div>
        <% if (results && results.length > 0) { %>
        <div class="table-responsive">
          <table>
            <thead>
              <tr>
                <th>PROVINCIA</th>
                <th>DISTRITO</th>
                <th>CORREGIMIENTO</th>
                <th>CENTRO</th>
                <th>CEDULA</th>
                <th>NOMBRE</th>
                <th>MESA</th>
                <th>SEXO</th>
                <th>EDAD</th>
                <th>F_INSCRIPCION</th>
                <th>ELECTORES</th>
                <th>Celular</th>
                <th>Celular 2</th>
              </tr>
            </thead>
            <tbody>
              <% results.forEach(function(record) { %>
              <tr>
                <td><%= record['ID'] %></td>
                <td><%= record['PROVINCIA'] %></td>
                <td><%= record['DISTRITO'] %></td>
                <td><%= record['CORREGIMIENTO'] %></td>
                <td><%= record['CENTRO'] %></td>
                <td><%= record['NOMBRE'] %></td>
                <td><%= record['MESA'] %></td>
                <td><%= record['SEXO'] %></td>
                <td><%= record['EDAD'] %></td>
                <td><%= record['F_INSCRIPCION'] %></td>
                <td><%= record['ELECTORES'] %></td>
                <td><%= record['Celular'] %></td>
                <td><%= record['Celular 2'] %></td>
              </tr>
              <% }); %>
            </tbody>
          </table>
        </div>
        <% } else { %>
        <p style="text-align: center; margin-top: 10px">
          No se encontraron resultados. Por favor, intenta con otra búsqueda.
        </p>
        <% } %>
        <div class="footer">
          <a href="/">Volver a la búsqueda</a>
        </div>
      </div>
    </div>
    
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script>
  </body>
</html>